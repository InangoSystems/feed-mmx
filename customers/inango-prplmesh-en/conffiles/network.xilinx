
#Configuration of network interfaces for DPU (based on Xilinx)

config interface loopback
        option ifname   lo
        option proto    static
        option ipaddr   127.0.0.1
        option netmask  255.0.0.0

config interface mgmt
        option ifname   eth0
        option proto    static
        option ipaddr   172.16.10.48
        option netmask  255.255.255.0
